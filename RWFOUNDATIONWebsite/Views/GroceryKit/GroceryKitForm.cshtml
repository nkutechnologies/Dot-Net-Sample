@model RWFOUNDATIONWebsite.DataModels.SaveAsDrafts.BeneficiaryFormSaveAsDraft
@{
    ViewData["Title"] = "GroceryKitForm";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
    string already = ViewBag.alreadyadded;
}


@if (already != null)
{
    <div class="contianer mt-5">
        <div class="row">
            <div class="col-md-6 offset-md-3">
                <div class="card">
                    <div class="p-4 p-sm-5">
                        <div class="display-1 lnr lnr-cross-circle text-center text-danger mb-4"></div>
                        <p class="text-center text-big mb-4">@already</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
else
{
    if (Model != null)
    {
        <section class="main-section  bg-light">
            <div class="container">
                <div class="appointment-wrap bg-white p-4 p-md-5  align-items-center">
                    <form id="grocerykitform" class="appointment-form" enctype="multipart/form-data">
                        <h3>Application for Grocery Kit</h3>
                        <input hidden id="grocerykitId" value="@Model.GroceryKitId" />
                        <input hidden id="existingimageurl" asp-for="@Model.ImageUrl" />
                        <input hidden id="existingcnicfronturl" asp-for="@Model.CNICFrontUrl" />
                        <input hidden id="existingcnicbackurl" asp-for="@Model.CNICBackUrl" />
                        <input hidden id="existingdisabilityurl" asp-for="@Model.DisabilityCertificateUrl" />
                        <input hidden id="existingdeathurl" asp-for="@Model.DeathCertificateUrl" />
                        <input hidden id="existingbformurl" asp-for="@Model.BFormUrl" />
                        <input hidden id="existingelectricityurl" asp-for="@Model.ElectricityBill" />
                        <input hidden id="existingptclurl" asp-for="@Model.PtclBillUrl" />
                        <input hidden id="existingappurl" asp-for="@Model.ApplicationUrl" />
                        <input hidden id="existingotherdoc1url" asp-for="@Model.OtherDocument1Url" />
                        <input hidden id="existingotherdoc2url" asp-for="@Model.OtherDocument2Url" />
                        <input hidden id="existingvideo" asp-for="@Model.VideoUrl" />
                        <input hidden id="existingaudio" asp-for="@Model.AudioUrl" />
                        <div class="row">
                            <div class="col-lg-4">
                                <label class="text-dark">First Name(پہلا  نام)</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="firstname" name="firstname" class="form-control" value="@Model.FirstName" placeholder="First Name">
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Last Name(آخری  نام)</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="lastname" name="lastname" class="form-control" value="@Model.LastName" placeholder="Last Name">
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Father/Husband Name(باپ  یا  خاوند کا  نام)</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="fatherorhusbandname" name="fatherorhusbandname" value="@Model.FatherOrHusbandName" class="form-control" placeholder="Father/ Husband Name">
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <label class="text-dark">Medical Probs( بیماری )</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select name="medicalprob" id="medicalprob" class="form-control" asp-for="@Model.MedicalProbId" asp-items="ViewBag.MedicalProb">
                                                <option data-id="" disabled>Select</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Phone Number 1(1 فون  نمبر)</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="phonenumber1" name="phonenumber1" value="@Model.PhoneNumber1" class="form-control" placeholder="(92) 333 0000000">
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Phone Number 2(2 فون  نمبر)</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="phonenumber2" name="phonenumber2" value="@Model.PhoneNumber2" class="form-control" placeholder="(92) 333 0000000">
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <label class="text-dark">CNIC( شناختی کارڈ نمبر)</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="cnic" name="cnic" class="form-control" value="@Model.CNIC" maxlength="15" placeholder="35202-0001110-0">
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Old Form No(پرانے فارم کا نمبر)</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="oldformno" name="oldformno" value="@Model.OldFormNo" class="form-control" placeholder="Enter Old Form Number">
                                    </div>
                                </div>

                            </div>

                            <div class="col-lg-4">
                                <label class="text-dark">Merital Status(ازدواجی حیثیت)</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select id="meritalstatus" name="meritalstatus" asp-for="@Model.MeritalStatus" class="form-control">
                                                <option value="Married">Married</option>
                                                <option value="Single">Single</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <label class="text-dark">Current Status(موجود حالت)</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select name="currentstatus" id="currentstatus" asp-for="@Model.CurrentStatusId" class="form-control" asp-items="ViewBag.CurrentStatus">
                                                <option data-id="" disabled>Select</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Gender</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select name="gender" id="gender" asp-for="@Model.Gender" class="form-control">
                                                <option value="Male">Male</option>
                                                <option value="Female">Female</option>
                                                <option value="Shemale">Shemale</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Residence Status</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select name="residencestatus" id="residencestatus" asp-for="@Model.ResidenceStatus" class="form-control">
                                                <option value="Rented">Rented</option>
                                                <option value="Own">Own</option>
                                                <option value="Others">Others</option>
                                                <option value="Homeless">Homeless</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <label class="text-dark">Occupation</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select name="occupation" id="occupation" asp-for="@Model.OccupationId" class="form-control" asp-items="ViewBag.Occupation">
                                                <option data-id="" disabled>Select</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Province</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select name="province" id="province" asp-for="@Model.ProvinceId" class="form-control" asp-items="ViewBag.Province">
                                                <option data-id="" disabled>Select</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">City</label>
                                <div class="form-group">
                                    <div class="form-field">
                                        <div class="select-wrap">
                                            <select name="city" id="city" asp-for="@Model.CityId" class="form-control" asp-items="ViewBag.City">
                                                <option data-id="" disabled>Select</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <label class="text-dark">Update Photo</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="file" id="photo" name="photo" class="form-control" placeholder="Add Photo">
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Date Of Birth?</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="date" id="dateofbirth" name="dateofbirth" value="@Model.DateOfBirth.ToString("yyyy-MM-dd")" class="form-control" placeholder="Date Of Birth" value="@DateTime.Now.ToString("yyyy-MM-dd")">
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                <label class="text-dark">Address</label>
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="text" id="address" name="address" value="@Model.Address" class="form-control" placeholder="Address">
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4">
                                @if (Model.ImageUrl != null)
                                {
                                    <img src="~/DraftImages/Draft_MainPhoto/@Model.ImageUrl" height="150" width="150" alt="Alternate Text" />
                                }
                                else
                                {
                                    <img src="~/assets/img/RW/userimage.png" height="150" width="150" alt="Alternate Text" />
                                }

                            </div>
                        </div>
                        <div class="text-center">
                            <h5 class="text-dark font-weight-bold text-danger">Family Members Details</h5>
                        </div>
                        <div class="row">
                            <div class="col-md-12 align-items-center">
                                <a onclick="AddNewRow()" class="btn btn-sm bg-warning float-right text-white "><i class="icofont-plus text-white mr-1"></i>Add New Member</a>
                                <table class="table table-light" id="GroceryKitTbl">
                                    <thead class="bg-warning text-white">
                                        <tr class="text-center">
                                            <th>Name</th>
                                            <th>Relation</th>
                                            <th>Age</th>
                                            <th>Status</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody class="text-center">
                                        @foreach (var item in Model.FamilyMembers)
                                        {
                                            <tr>
                                                <td>
                                                    <div class="form-group">
                                                        <div class="input-wrap">
                                                            <input type="text" value="@item.Name" class="form-control" placeholder="Name">
                                                        </div>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-field">
                                                        <div class="select-wrap">
                                                            <select name="relationupdate" class="form-control" asp-items="ViewBag.Relations" asp-for="@item.RelationId">
                                                            </select>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        <div class="input-wrap">
                                                            <input type="number" value="@item.Age" class="form-control" placeholder="Age">
                                                        </div>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-field">
                                                        <div class="select-wrap">
                                                            <select class="form-control" asp-items="ViewBag.FamilyMemberStatus" asp-for="@item.FamilyMemberStatusId">
                                                                <option data-id="" disabled>Select</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td><a title="Delete Row" onclick="deleteRow(this)" class="btn btn-sm bg-danger border-0 text-white"><i class="feather icon-trash-2"></i></a></td>
                                            </tr>
                                        }
                                        <tr class="sampleRow" style="display: none" id="row">
                                            <td>
                                                <div class="form-group">
                                                    <div class="input-wrap">
                                                        <input type="text" id="familymembername" class="form-control" placeholder="Name">
                                                    </div>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="form-field">
                                                    <div class="select-wrap">
                                                        <select name="relation" id="relation" class="form-control" asp-items="ViewBag.Relations">
                                                            <option data-id="" disabled>Select</option>

                                                        </select>
                                                    </div>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="form-group">
                                                    <div class="input-wrap">
                                                        <input type="number" id="familymemberage" value="0" class="form-control" placeholder="Age">
                                                    </div>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="form-field">
                                                    <div class="select-wrap">
                                                        <select name="familymemberstatus" id="familymemberstatus" class="form-control" asp-items="ViewBag.FamilyMemberStatus">
                                                            <option data-id="" disabled>Select</option>

                                                        </select>
                                                    </div>
                                                </div>
                                            </td>
                                            <td><a title="Delete Row" onclick="deleteRow(this)" class="btn btn-sm bg-danger border-0 text-white"><i class="feather icon-trash-2"></i></a></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="text-center mt-3 mb-3">
                            <h5 class="text-dark font-weight-bold text-danger">Short Fall In Cash Calculator</h5>
                        </div>
                        <div class="row">
                            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Food Cost:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="foodcost" class="form-control" value="@Model.FoodCost" placeholder="00.00">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">House Rent:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="houserent" value="@Model.HouseRent" class="form-control" placeholder="00.00">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">School Cost:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="schoolcost" value="@Model.SchoolCost" class="form-control" placeholder="00.00">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Utilities Cost:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="utilitiescost" value="@Model.UtilitiesCost" class="form-control" placeholder="00.00">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Medical Cost:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="medicalcost" value="@Model.MedicalCost" class="form-control" placeholder="00.00">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Other Cost:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="othercost" value="@Model.OtherCost" class="form-control" placeholder="00.00">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bolder">Total Cost:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="totalcost" value="@Model.TotalCost" class="form-control" placeholder="00.00" disabled>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Family Size:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="familysize" value="@Model.FamilySize" class="form-control" placeholder="Total No Of Family Member" disabled>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Salary:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="salary" value="@Model.Salary" class="form-control" placeholder="00.00" required>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Donations:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="donations" value="@Model.Donations" class="form-control" placeholder="00.00" required>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bold">Other Income:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="otherincome" value="@Model.OtherCost" class="form-control" placeholder="00.00" required>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-5 col-5">
                                        <label class="text-dark font-weight-bolder">Total income:</label>
                                    </div>
                                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-7 col-7">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="number" id="totalincome" value="@Model.TotalIncome" class="form-control" placeholder="00.00" disabled>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xl-3 col-lg-3 col-md-3 col-sm-6 col-6">
                                <label class="font-weight-bolder text-danger">Short Fall In Cash</label>
                            </div>
                            <div class="col-xl-9 col-lg-9 col-md-9 col-sm-6 col-6">
                                <div class="form-group">
                                    <div class="input-wrap">
                                        <input type="number" id="shortfallcash" value="@Model.ShortFallInCash" class="form-control" placeholder="000.000" disabled>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="text-center">
                            <h5 class="text-dark font-weight-bold text-danger">Attach Multiple Documents Here</h5>
                        </div>
                        <div class="">
                            <div class="form-group">
                                <div class="input-wrap">
                                    <input type="text" class="form-control" disabled>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-6 col-6">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input class="form-control" value="Upload CNIC Front" disabled>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xl-8 col-lg-8 col-md-8 col-sm-11 col-11">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="file" id="CNICFRONT" class="form-control" placeholder="Upload CNIC Front">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xl-1 col-lg-1 col-md-1 col-sm-1 col-1">
                                        <a id="cnicviewfront" data-id="" class='pdfbutton text-center text-warning font-weight-bolder' title="View CNIC Front"> <i class='feather icon-eye' aria-hidden='true'></i></a>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-12 col-12">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input class="form-control" value="Upload CNIC Back" disabled>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xl-8 col-lg-8 col-md-8 col-sm-11 col-11">
                                        <div class="form-group">
                                            <div class="input-wrap">
                                                <input type="file" id="CNICBACK" class="form-control" placeholder="Upload CNIC Back">
                                            </div>
                                        </div>
